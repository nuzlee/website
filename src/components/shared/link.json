{
  "collectionName": "components_shared_links",
  "info": {
    "displayName": "link"
  },
  "options": {},
  "attributes": {
    "label": {
      "type": "string"
    },
    "href": {
      "type": "string"
    },
    "icon": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "isButton": {
      "type": "boolean",
      "default": false
    },
    "isExternal": {
      "type": "boolean",
      "default": false
    },
    "backgroundColor": {
      "type": "customField",
      "customField": "plugin::color-picker.color",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$"
    }
  },
  "config": {}
}
